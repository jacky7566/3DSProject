{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=10.21.166.20;Initial Catalog=eDoc_Dev;Persist Security Info=True;User ID=cimsql;Password=roadtrip;Connection Timeout=1800;TrustServerCertificate=True"
  },
  "ServerName": "",
  "MailSettings": {
    "mailTitle": "eDoc Ingestion Alert",
    "sendMail": "grading_sys@winfoundry.com",
    "smtpServer": "relay.li.lumentuminc.net",
    "receiveMails": "jacky.li@lumentum.com"
  },
  "Configurations": {
    "IsMoveToIngestedFolder": "Y",
    "ProductFilter": "PB21*.txt|PA23*.txt|P621*.txt|P721*.txt|E1Z1*.txt|E0E0*.txt|E1E1*.txt|P1E1*.txt|EJ20*.txt|PJ20*.txt|E0D0*.txt|PA4B*.txt",
    "DefaultEMapVersion": "eMap V2.90",
    "IsDebug": "True",
    "SQL_CommandTimeout": "180",
    "SpecialByPass": "True"
  },
  "AOI2_Detector": {
    "HOR_VER_BIN_GREATER": "71",
    "HOR_VER_BIN_EXCLUDE": "77,93,103,104,105,85,99",
    "LIMIT": "4"
  },
  "NLog": {
    "autoReload": true,
    "throwConfigExceptions": false,
    "internalLogLevel": "info",
    "internalLogFile": "${basedir}/logs/internal-nlog/internal-nlog.txt",
    "targets": {
      "logfile": {
        "type": "File",
        "fileName": "${basedir}/logs/NLogDeml${shortdate}.log",
        "layout": "${date} [${uppercase:${level}}] ${message} ${exception}${newline}"
      },
      "logconsole": {
        "type": "Console",
        "layout": "${date} [${uppercase:${level}}] ${message} ${exception}"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "logfile,logconsole"
      }
    ]
  }
}