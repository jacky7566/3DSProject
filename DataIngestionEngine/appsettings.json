  "ConnectionStrings": {
    "DefaultConnection": "Data Source=taogramssql01;Initial Catalog=eDoc_Dev;Persist Security Info=True;User ID=cimsql;Password=roadtrip;Connection Timeout=1800;TrustServerCertificate=True"
  },
  "ServerName": "TYCMISGD03",
  "MailSettings": {
    "mailTitle": "eDoc Ingestion Alert",
    "sendMail": "grading_sys@winfoundry.com",
    "smtpServer": "relay.li.lumentuminc.net",
    "receiveMails": "jacky.li@lumentum.com"
  },
  "Configurations": {
    "IsMoveToIngestedFolder": "Y",
    "ProductFilter": "PB21*.txt|PA23*.txt|P621*.txt|P721*txt"
  },
  "NLog": {
    "autoReload": true,
    "throwConfigExceptions": false,
    "internalLogLevel": "info",
    "internalLogFile": "${basedir}/logs/internal-nlog/internal-nlog.txt",
    "targets": {
      "logfile": {
        "type": "File",
        "fileName": "${basedir}/logs/NLogDeml${shortdate}.log",
        "layout": "${date} [${uppercase:${level}}] ${message} ${exception}${newline}"
      },
      "logconsole": {
        "type": "Console",
        "layout": "${date} [${uppercase:${level}}] ${message} ${exception}"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "logfile,logconsole"
      }
    ]
  }
}